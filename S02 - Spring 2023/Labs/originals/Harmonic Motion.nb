   RowBox[{"AxesLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<time\>\"", ",", "\"\<Energy\>\""}], "}"}]}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.502620952277624*^9, 3.502620955716208*^9}, {
  3.502621121965045*^9, 3.5026211329390993`*^9}, {3.5026211864227448`*^9, 
  3.502621263168231*^9}},
 AspectRatioFixed->True,
 CellLabel->"In[22]:=",ExpressionUUID->"5cfe6ca1-59ae-4f68-ad16-5b1a5ebf41b0"],

